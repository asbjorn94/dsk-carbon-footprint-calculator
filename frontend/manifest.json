{
    "manifest_version": 3,
    "name": "Den Store Klimadatabase Extension",
    "version": "1.0",
    "permissions": ["https://asbjorn.eu.pythonanywhere.com/"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ]
  }